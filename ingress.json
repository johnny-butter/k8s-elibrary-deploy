{
	"apiVersion": "networking.k8s.io/v1beta1",
	"kind": "Ingress",
	"metadata": {
		"name": "elibrary-ingress",
		"namespace": "elibrary"
	},
	"spec": {
		"rules": [
			{
				"host": "elibrary.com",
				"http": {
					"paths": [
						{
							"backend": {
								"serviceName": "app-service",
								"servicePort": 8000
							}
						}
					]
				}
			}
		]
	}
}
